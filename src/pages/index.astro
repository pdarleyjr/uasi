// Helper function to extract YouTube URL from content
function extractYoutubeUrl(content: string): string {
  if (!content) return '';
  const lines = content.split('\n');
  const urlLine = lines.find(line => line.includes('youtube.com'));
  if (!urlLine) return '';
  return urlLine.trim();
}


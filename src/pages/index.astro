<Layout title="UASI Equipment Catalog">
  <header class="bg-white shadow-sm py-4 sticky top-0 z-50">
    <div class="container mx-auto px-3 sm:px-4 md:px-6">
      <div class="flex flex-col md:flex-row items-center justify-between gap-3 md:gap-6">
        <img 
          src="/logo edited.png" 
          alt="UASI Logo" 
          class="h-12 sm:h-16 md:h-24 w-auto object-contain"
        />
        <div class="flex-1">
          <div class="bg-gray-50 p-2 sm:p-2.5 rounded-lg shadow-inner">
            <label for="equipmentSelect" class="block text-sm font-medium text-gray-600 mb-0.5 sm:mb-1">
              Equipment Selection
            </label>
            <p class="text-xs text-gray-400 mb-2">
              Select equipment from the list below to view details.
            </p>
            <div class="relative">
              <div class="absolute inset-y-0 right-0 flex items-center px-3 pointer-events-none">
                <svg class="w-4 h-4 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path></svg>
              </div>
              <select
                class="block w-full px-3 py-3 sm:py-2 pr-10 text-base sm:text-sm border border-gray-200 rounded-md focus:outline-none focus:ring-2 focus:ring-black focus:border-transparent bg-white cursor-pointer transition-colors hover:bg-gray-50"
                id="equipmentSelect"
                size="1"
                aria-label="Select equipment items"
              >
              <option value="">-- Select Equipment --</option>
              {equipment.map(item => (
                <option value={item.name}>
                  {item.name}
                </option>
              ))}
            </select>
            </div>
          </div>
        </div>
      </div>
    </div>
  </header>

  <main class="container mx-auto py-6 sm:py-8 md:py-12 px-3 sm:px-4 md:px-6">
    <div class="equipment-grid space-y-8 max-w-6xl mx-auto" id="equipmentGrid">
      {equipment.map(item => (
        <div class="equipment-card hidden" data-name={item.name}>
          <div class="bg-white rounded-xl shadow-lg overflow-hidden transform transition-all duration-300">
            <EquipmentCard
              name={item.name}
              image={item.image}
              quickReference={item.quickReference}
              technicalDetails={item.technicalDetails}
              safetyPractices={item.safetyPractices}
              pdfUrl={item.pdfUrl}
              pdfCarousel={item.pdfCarousel}
              youtubeUrl={item.youtubeUrl}
              additionalTabs={item.additionalTabs || []}
            />
          </div>
        </div>
      ))}
    </div>
    <div id="noSelection" class="text-center py-16">
      <div class="max-w-md mx-auto bg-gray-50 rounded-xl p-8 shadow-sm">
        <svg class="w-12 h-12 md:w-16 md:h-16 mx-auto mb-4 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
        </svg>
        <p class="text-gray-600 text-lg">
          Select equipment from the dropdown above to view details.
        </p>
      </div>
    </div>
  </main>
</Layout>

<script>
  function setupEquipmentSelect() {
    const select = document.getElementById('equipmentSelect') as HTMLSelectElement;
    const equipmentCards = Array.from(document.querySelectorAll('.equipment-card'));
    const noSelection = document.getElementById('noSelection');

    if (!select || !equipmentCards || !noSelection) return;

    // Hide all cards initially
    equipmentCards.forEach(card => {
      card.classList.add('hidden');
    });

    function updateDisplay() {
      const selectedValue = select.value;
      const selectedCard = equipmentCards.find(card => card.getAttribute('data-name') === selectedValue);

      // Hide all cards first
      equipmentCards.forEach(card => {
        card.classList.add('hidden');
      });

      if (selectedCard) {
        selectedCard.classList.remove('hidden');
        noSelection.classList.add('hidden');
        // Smooth scroll to the card
        selectedCard.scrollIntoView({ behavior: 'smooth', block: 'start' });
      } else {
        noSelection.classList.remove('hidden');
      }
    }

    // Handle selection changes
    select.addEventListener('change', updateDisplay);
  }

  // Setup equipment select on page load
  setupEquipmentSelect();

  // Setup equipment select when content updates
  document.addEventListener('astro:page-load', setupEquipmentSelect);
  document.addEventListener('astro:after-swap', setupEquipmentSelect);
</script>

<style>
  .hidden {
    display: none;
  }

  select[multiple] {
    height: auto;
  }

  select {
    font-size: 1rem;
  }

  select option {
    padding: 1rem;
    font-size: 1rem;
    margin: 0.25rem 0;
    border-radius: 0.25rem;
    transition: all 0.2s;
  }

  select option:checked {
    background-color: rgb(59 130 246) !important;
    color: white;
  }

  select option:hover {
    background-color: #f3f4f6;
  }

  /* Custom scrollbar for the select element */
  select {
    scrollbar-width: thin;
    scrollbar-color: #cbd5e1 #f8fafc;
  }

  select::-webkit-scrollbar {
    width: 8px;
  }

  select::-webkit-scrollbar-track {
    background: #f8fafc;
    border-radius: 4px;
  }

  select::-webkit-scrollbar-thumb {
    background-color: #cbd5e1;
    border-radius: 4px;
  }

  select::-webkit-scrollbar-thumb:hover {
    background-color: #94a3b8;
  }

  /* Equipment card transitions and mobile optimization */
  .equipment-card {
    transition: all 0.3s ease-in-out;
    opacity: 0;
    transform: translateY(20px);
  }
  
  .equipment-card.hidden {
    display: none;
  }

  .equipment-card:not(.hidden) {
    opacity: 1;
    transform: translateY(0);
  }

  /* Mobile optimizations */
  @media (max-width: 768px) {
    select option {
      padding: 1rem;
    }
    .equipment-card:not(.hidden) {
      margin-bottom: 2rem;
    }
  }
</style>
